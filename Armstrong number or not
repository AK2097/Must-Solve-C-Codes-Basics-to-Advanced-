#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,temp, rem, sum = 0;
    cout<<"enter a number" <<endl;
    cin>>n;
    temp = n;
    while(temp!=0)
    {
        rem = temp % 10;
        sum = sum + rem * rem * rem;
        temp = temp/10;
    }
    if(sum == n)
    cout<<"armstrong number" <<endl;
    else
    cout<<"not a armstrong number" <<endl;
    return 0;
}


enter a number
371
armstrong number
