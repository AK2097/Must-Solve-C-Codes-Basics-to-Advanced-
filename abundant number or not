#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,sum=0;
    cout<<"enter a number" <<endl;
    cin>>n;
    int i;
    for(i=1; i<n; i++)
    {
        if(n%i==0)
        {
            sum = sum+i;
        }
    }
    if(n<sum)
    cout<<"abundant number" <<endl;
    else
    cout<<"not an abudant number" <<endl;
    return 0;
}


enter a number
12
abundant number
