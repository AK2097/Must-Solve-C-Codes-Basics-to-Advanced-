#include<bits/stdc++.h>
using namespace std;
int main()
{
    char str1[60], str2[60];
    cout<<"enter the first string and the second string" <<endl;
    cin>>str1>>str2;
     if(strcmp(str1,str2) == 0)
     cout<<"strings are equal" <<endl;
     else
     cout<<"strings are not equal" <<endl;
     return 0;
}
