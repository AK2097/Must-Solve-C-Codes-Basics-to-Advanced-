#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    int count = 0;
    cout<<"enter a number" <<endl;
    cin>>n;
    int i;
    for(i=1; i<=n; i++)
    {
        if(n%i==0)
        {
        count++;
        cout<<i<<endl;
        }
    }
    
    cout<<"total factors is : " << " " <<count<<endl;
    return 0;
}

enter a number
60
1
2
3
4
5
6
10
12
15
20
30
60
total factors is :  12
