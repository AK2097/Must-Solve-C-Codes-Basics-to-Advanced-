#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n, a = 0, b = 1, sum =0;
    cout<<"enter a number" <<endl;
    cin>>n;
    for(int i=1; i<=n; i++)
    {
       
            cout<<sum<<" ";
            a = b;
            b = sum;
            sum = a+b;
        }
        
    
    return 0;
    }
    
    
    enter a number
20
