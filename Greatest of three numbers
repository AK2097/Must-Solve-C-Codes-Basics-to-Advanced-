#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c;
    cout<<"Enter three numbers" <<endl;
    cin>>a>>b>>b;
    if(a>b && a>c)
    {
        cout<<"a is greater" <<endl;
    }
    else if(b>a && b>c)
    {
        cout<<"b is greater" <<endl;
    }
    else
    {
        cout<<"c is greater" <<endl;
    }
    return 0;
}
    
    
    Enter three numbers
10 20 30
c is greater
