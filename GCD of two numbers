#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,gcd;
    cout<<"Enter two numbers" <<endl;
    cin>>a>>b;
    int i;
    for(i=1; i<=a && i<=b; i++)
    {
        if((a%i==0) && (b%i==0))
        {
            gcd = i;
        }
    }
    cout<<"gcd of two numbers is" <<" " << gcd << endl;
    return 0;
}


    Enter two numbers
20 28
gcd of two numbers is 4
