#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b;
    cout<<"Enter two numbers" <<endl;
    cin>>a>>b;
    int lcm = (a>b)?a:b;
    while(1)
    {
        if(lcm%a==0 && lcm%b==0)
        {
        cout<<lcm<<endl;
        break;
        }
        ++lcm;
    }

    return 0;
}


Enter two numbers
7 21
21
